<%= render 'nav' %>
<h3>Deactivated Promoter</h3>
<section id="records">
<table border="1">
	<tr>
    <th>Emailid</th>
    <th>Company</th>
    <th>Mobile</th>
    <th>Actions</th>
	</tr>
	<% count = 1 %>
	<% @iusers.each do |user| %>		
		<tr>
			<td><%= user.email %></td>
      <td><%= user.partner.try(:company_name) %></td>
			<td><%= user.mobile %></td>
			<td> <%= link_to 'Activate', "/refinery/admin/bank_details/#{user.id}/activate_client" %></td>
			<% count = count + 1 %>
		</tr>
	<% end %>
</table>
</section>
